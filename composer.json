{
  "name": "ischenko/yii2-jsloader-requirejs",
  "description": "An Yii2 extension which allows to handle asset bundles by requirejs",
  "keywords": [
    "yii2",
    "view",
    "requirejs",
    "behavior",
    "assets"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Roman Ishchenko",
      "email": "roman@ishchenko.ck.ua"
    }
  ],
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.4.0",
    "ischenko/yii2-jsloader": "dev-master",
    "bower-asset/requirejs": "^2.3"
  },
  "require-dev": {
    "codeception/verify": "*",
    "codeception/specify": "*",
    "codeception/codeception": "*"
  },
  "autoload": {
    "psr-4": {
      "ischenko\\yii2\\jsloader\\": "src/"
    }
  }
}
